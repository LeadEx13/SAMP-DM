public OnPlayerCommandText(playerid,cmdtext[])
{
	//================Tele====================
	if(strcmp(cmdtext,"/stunts2",true)==0)
	{
	   if(GetPlayerState(playerid) == 2)
	   {
	       SetVehiclePos(GetPlayerVehicleID(playerid),1788.1700,-2455.4468,13.5547);
	       SetVehicleZAngle(GetPlayerVehicleID(playerid),225.3882);
	   }
	   else
	   {
	       SetPlayerInterior(playerid,0);
	       SetPlayerPos(playerid,1788.1700,-2455.4468,13.5547);
	       SetPlayerFacingAngle(playerid,225.3882);
	   }
	   SendClientMessage(playerid,0xFFFF00AA,".ברוך הבא לאזור הסטאנטים");
	   return 1;
	}
	
	//================Open====================
	if(strcmp(cmdtext,"/open moto",true) == 0) return MoveObject(object,1891.922852,-2430.610840,15.312510,4);
	
	//===============Close====================
	if(strcmp(cmdtext,"/close moto",true) == 0) return MoveObject(object,1903.891846,-2431.514160,15.312510,4);
	return 0;
}
