forward StartNFSActivity(time,type);
public StartNFSActivity(time,type)
{
	new i;
	if(time > 0)
	{
		new string[25];
		format(string, sizeof(string),"~w~%d",time);
		for (i=0; i<MAX_PLAYERS; i++)
		{
		    if (IsPlayerConnected(i) && players[i])
		    {
				PlayerPlaySound(i,1058, 0.0, 0.0, 0.0);
				GameTextForPlayer(i,string,1000,5);
			}
		}
		time--;
		SetTimerEx("StartNFSActivity",1000,false,"i",time);
	}
	else
	{
		for (i=0; i<MAX_PLAYERS; i++)
		{
		    if (IsPlayerConnected(i) && players[i])
		    {
				TogglePlayerControllable(i,true);
				PlayerPlaySound(i,1057, 0.0, 0.0, 0.0);
				GameTextForPlayer(i,"~g~Go !! ~r~Go !! ~b~Go !!",4000,5);
			}
		}
		NFS_Timer = SetTimer("CheckPlayersStatus",500,true);
	}
}


forward CheckPlayersStatus();
public CheckPlayersStatus()
{
	new i, stats, string[128];
	for (i=0; i<MAX_PLAYERS; i++)
	{
	    if (!IsPlayerConnected(i) && players[i])
	    {
			players[i] = false;
			DestroyVehicle(vehicles[i]);
		}
	    else
	    if (players[i])
	    {
	    	stats = GetPlayerState(i);
	    	if (stats != 2 && stats != 7)
	    	{
	        	SendClientMessage(i,LIGHT_BLUE," ! נפסלת מהפעילות מירוץ המוות ");
	        	DisablePlayerRaceCheckpoint(i);
	        	DestroyVehicle(vehicles[i]);
                SpawnPlayer(i);
				players[i] = false;
	    	}
	    }
	}

	if (GetActivePlayers() == 1)
	{
	    KillTimer(NFS_Timer);
	    for (i=0; i<MAX_PLAYERS; i++)
	    {
	        if (players[i])
	        {
	   			new name[25];
	   			name = GetName(i);
				format(string,sizeof(string),"!! קיבל 50,000 דולר %s !! הוא המנצח של מירוץ המוות %s",name,name);
				SendClientMessageToAll(YELLOW,string);
                players[i] = false;
                DisablePlayerRaceCheckpoint(i);
				DestroyVehicle(vehicles[i]);
				SpawnPlayer(i);
				SetTimerEx("GivePlayerActMoney",2000,false,"ii",i,50000);
                ResetNFS(true);
     			break;
     		}
    	}
	}
	else
	{
		if (GetActivePlayers() == 0)
		{
		    KillTimer(NFS_Timer);
		    ResetNFS(true);
			SendClientMessageToAll(YELLOW," !! קרה מצב של תיקו בין שני שחקנים לכן לא נקבע מנצח ");
		}
	}
}

ResetNFS(mode)
{
	new i;
	NFS = false;
	KillTimer(NFS_Timer);
	KillTimer(Barrels_Timer);
	for (i=0; i<MAX_PLAYERS; i++)
	{
		if (players[i])
		{
		    players[i] = false;
			DisablePlayerRaceCheckpoint(i);
			DestroyVehicle(vehicles[i]);
			if (mode) SpawnPlayer(i);
		}
	}

	DestroyBarrels();
	DestroyBikes();
	DestroyStaticObjects();
}

GetActivePlayers()
{
	new num = 0, i;
	for (i=0; i<MAX_PLAYERS; i++) if (players[i]) num++;
	return num;
}

