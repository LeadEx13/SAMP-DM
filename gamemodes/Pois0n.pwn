forward PiraCountDown(time);
public PiraCountDown(time)
{
	new i;
	if(time > 0)
	{
		new string[25];
		format(string, sizeof(string),"~g~PIRA : %s%d",(time > 30) ? ("~b~") : ("~r~"),time);
		for (i=0; i<MAX_PLAYERS; i++) if (IsPlayerConnected(i)) PlayerPlaySound(i,1058, 0.0, 0.0, 0.0);
		GameTextForAll(string,1000,6);
		time--;
		SetTimerEx("PiraCountDown",1000,false,"i",time);
	}
	else
	{
	    CanJoin = false;
	    if (GetActivePlayers() > 1)
	    {
		    for (i=0; i<MAX_PLAYERS; i++)
		    {
				if (IsPlayerConnected(i) && players[i])
				{
				    TeleportRandomPiramida(i);
					SendClientMessage(i,GREEN," * * * * * * * * * ברוכים הבאים לפעילות פירמידת גבהים * * * * * * * * * *");
					SendClientMessage(i,LIGHT_BLUE,"* * * * * המטרה שלכם היא להיות הגבוהים ביותר לאחר שיגמר הזמן * * * * * *");
					SendClientMessage(i,GREEN," * * * * * * * * !! השחקן הגבוה ביותר יזכה ב - 50,000 דולר * * * * * * * * * ");
				    SetPlayerWorldBounds(i,2378.2439,2261.5251,1355.2419,1218.9800);
				}
      		}
		    SetTimerEx("RunPiramidaActivity",2000,false,"i",120);
		}
		else
		{
			Piramida = false;
			for (i=0; i<MAX_PLAYERS; i++)
			{
				players[i] = false;
				oldPos[i][0] = 0;
				oldPos[i][1] = 0;
				oldPos[i][2] = 0;
			}
			for (i=0; i<sizeof(objects); i++)
				F_DestroyObject(objects[i]);
			SendClientMessageToAll(GREEN," הפעילות לא יכולה להתחיל עם פחות משני שחקנים ");
		}
	}
}

