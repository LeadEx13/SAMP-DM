public PhoneCut()
{
	for (new i = 0; i < GetMaxPlayers(); i++){
		if (IsPlayerConnected(i)){
			if (Calling[i] > -1 && Answered[i] == 1 && Callerid[i] == 1){
				if (FAC_GetPlayerMoney(i) >= CallCost){
				GetPlayerName(i,iname,sizeof(iname));
				if(strcmp(dini_Get(PlayerFile(iname),"Organization"),"Supporters",true)){
				FAC_GivePlayerMoney(i, -CallCost);
				}
				}
				if (FAC_GetPlayerMoney(i) < CallCost){
					SendClientMessage(i, COLOR_BRIGHTRED, ".השיחה נותקה... מכיוון שאין לך מספיק כסף בכדי להמשיכה");
					SendClientMessage(Calling[i], COLOR_BRIGHTRED, "..השיחה נותקה מכיוון שלמתקשר נגמר הכסף");
					Calling[Calling[i]] = -1;
					Answered[Calling[i]] = 0;
					Calling[i] = -1;
					Answered[i] = 0;
					Callerid[i] = 0;
				}}}}
	return 1;
}


