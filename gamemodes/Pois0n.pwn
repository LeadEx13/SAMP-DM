forward NFSCountDown(time);
public NFSCountDown(time)
{
	new i;
	if(time > 0)
	{
		new string[25];
		format(string, sizeof(string),"~R~NFS : %s%d",(time > 30) ? ("~b~") : ("~r~"),time);
		for (i=0; i<MAX_PLAYERS; i++) if (IsPlayerConnected(i)) PlayerPlaySound(i,1058, 0.0, 0.0, 0.0);
		GameTextForAll(string,1000,6);
		time--;
		SetTimerEx("NFSCountDown",1000,0,"i",time);
	}
	else
	{
	    CanJoin = false;
	    if (GetActivePlayers() > 1)
		{
		    CreateBarrels();
		    CreateBikesPutPlayers();
		    for (i=0; i<MAX_PLAYERS; i++)
				if (IsPlayerConnected(i) && players[i])
				{
					SetPlayerRaceCheckpoint(i,4,1350.1152,-2489.4216,21.5375,1282.0624,-2492.6616,21.5375,10);
					SendClientMessage(i,GREEN," * * * * * * * * * ברוכים הבאים למירוץ המוות * * * * * * * * * *");
					SendClientMessage(i,LIGHT_BLUE,"* * * * * המטרה שלכם היא להגיע לסוף המסלול בלי להתפוצץ * * * * * *");
					SendClientMessage(i,GREEN," * * * * * * * * המנצח זוכה בפרס של 50,000 דולר * * * * * * * * * ");
				}
			Barrels_Timer = SetTimer("MoveBarrels",1000,true);
			SetTimerEx("StartNFSActivity",2000,false,"i",10);
		}
		else
		{
		    ResetNFS(false);
