{
	for(new i; i < 200; i++)
	{
		if(IsPlayerConnected(i) && In_QuaDRace[i] == 1)
		{
		TogglePlayerControllable(i, 1);
		SendClientMessage(i,COLOR_LIGHTGREEN, "- QuaD: המירוץ החל, בהצלחה - שלושת המקומות הראשונים יזכו בפרסים");
		}
	}
	SendClientMessageToAll(COLOR_WHITE, "--- QuaD ~ QuaD מירוץ ה ---");
	SendClientMessageToAll(COLOR_LIGHTGREEN, ".המירוץ החל, בהצלחה לכולם");
	format(PosString, sizeof(PosString),".%d :מספר משתתפים",QuaDRace_Players);
 	SendClientMessageToAll(COLOR_LIGHTGREEN, PosString);
 	SendClientMessageToAll(COLOR_WHITE, "--- .בהצלחה במירוץ ---");
	GameTextForAll("~g~~h~~h~QuaDRace Started !",3000,6);
