public MoneyScoreUpdate()
{
	new CashScore;
	new name[MAX_PLAYER_NAME];
	for(new i = 0; i < GetMaxPlayers(); i++)
	{
		if (IsPlayerConnected(i))
		{
			GetPlayerName(i, name, sizeof(name));
   			CashScore = FAC_GetPlayerMoney(i);
			ResetPlayerMoney(i);
			GivePlayerMoney(i,FAC_GetPlayerMoney(i));
			SetPlayerScore(i, CashScore);
			if (CashScore > CashScoreOld){ CashScoreOld = CashScore; }
		}
	}
}
//==============================================================================
public OnPlayerSelectedMenuRow(playerid, row)
{

 
	new Menu:Current;
    Current = GetPlayerMenu(playerid);
    
 
	  if(Current == Games)
	{
	    if (!(MiniOn || GreOn || NFS || WarOn || RaceBarron || DozerOn || ClimbOn || MonsterOn || Piramida || GlassOn ||Sandking_On || SAN_On || BanD_On || Vor_On || SHIP_On || CH || MavohOn || OlimpOn || MkillOn || ChainSawOn || glasson || Cycle_On || NrgRace_On || RCBanditRace_On || QuaDRace_On))
	    {
		    switch (row)
		    {
		        case 0 : OnPlayerCommandText(playerid,"/smini");
		        case 1 : OnPlayerCommandText(playerid,"/sgre");
		        case 2 : ActiveNFS(playerid);
		        case 3 : OnPlayerCommandText(playerid,"/swar");
		        case 4 : ActivePiramida(playerid);
		        case 5 : OnPlayerCommandText(playerid,"/sdz");
                case 6 : OnPlayerCommandText(playerid,"/scb");
                case 7 : OnPlayerCommandText(playerid,"/smon");
                case 8 : ActiveBarron(playerid);
                case 9 : OnPlayerCommandText(playerid,"/sGlass");
             //   case 10 : OnPlayerCommandText(playerid,"/Cstart");
				default : TogglePlayerControllable(playerid,true);
		    }
	    }
		else SendClientMessage(playerid,RED," !! אתה לא יכול להפעיל יותר מפעילות אחת, אנא חכה שתסתיים הפעילות הנוכחית ");
	    TogglePlayerControllable(playerid,true);
