    Vehicle_QuaDRace[seat] = CreateVehicle(471,QuaD_Pos[seat][0],QuaD_Pos[seat][1],QuaD_Pos[seat][2],QuaD_Pos[seat][3],QuaDRace_Color,QuaDRace_Color,999999);
    AddVehicleComponent(Vehicle_QuaDRace[seat],1010);
    AddVehicleComponent(Vehicle_QuaDRace[seat],1087);
		}
	new bla = 0;
	for(new i; i < 200; i++)
	{
		if(IsPlayerConnected(i) && In_QuaDRace[i] == 1)
		{
			PutPlayerInVehicle(i,Vehicle_QuaDRace[bla],0);
			Player_QuaDRace[i] = bla;
		    bla++;
			ResetPlayerWeapons(i);
			SetPlayerHealth(i, 100.0);
			TogglePlayerControllable(i, 0);
			SetPlayerInterior(i,0);
       	    SetPlayerRaceCheckpoint(i,0,QuaD_CheckPoints[0][0],QuaD_CheckPoints[0][1],QuaD_CheckPoints[0][2],QuaD_CheckPoints[1][0],QuaD_CheckPoints[1][1],QuaD_CheckPoints[1][2],10);
		}
	}
	return 1;
}

