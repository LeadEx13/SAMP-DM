	stock IsPlayerInCircle(playerid,Float:x,Float:y,Float:radius)
{
	if(GetPlayerDistanceToPointT(playerid,x,y) < radius) return 1;
	return 0;
}

stock GetPlayerDistanceToPointT(playerid,Float:x,Float:y)
{
