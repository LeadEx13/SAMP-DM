SendBotMessage(WarWinner,Bot_MSG);
SendClientMessage(WarWinner, COLOR_LIGHTBLUEGREEN, String);
for(new i;i<GetMaxPlayers();i++)if(IsPlayerConnected(i))InWar[i] = 0;
WarOn = 0,WarPlayers = 0,WarStarted = 0;
KillTimer(WarWinnerCheck);
return 1;
}

else if(status == 2) {
WarPlayers = 0;
WarStarted = 0;
WarOn = 0;
