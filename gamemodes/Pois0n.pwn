	    SendClientMessageToAll(YELLOW,string);
FAC_GivePlayerMoney(playerid,50000);
		Piramida = false;
		for (i=0; i<MAX_PLAYERS; i++)
		{
		    if (players[i])
			{
				ResetPlayerWeapons(i);
				players[i] = false;
				SpawnPlayer(i);
