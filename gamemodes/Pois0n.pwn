}
//LACY_GiveWeapon(i,35,1000000);
public ChainSawEnd(status) {
if(status == 0) {
SendClientMessageToAll(COLOR_WHITE, "[ChainSaw System] .בוטלה עקב מחסור במשתתפים [ChainSaw] - פעילות ה");
ChainSawOn = 0,ChainSawPlayers = 0,KillTimer(ChainSawWinnerCheck),ChainSawStarted = 0;
for(new i;i<GetMaxPlayers();i++){
if(IsPlayerConnected(i))InChainSaw[i] = 0;
}
return 1;
}
else if(status == 1) {
new ChainSawWinnerName[MAX_PLAYER_NAME];
GetPlayerName(ChainSawWinner, ChainSawWinnerName, sizeof(ChainSawWinnerName));
LACY_ResetWeapons(ChainSawWinner);
SetPlayerInterior(ChainSawWinner, 0);
SetPlayerRandomSpawn(ChainSawWinner);
dini_IntSet(PlayerFile(ChainSawWinnerName),"ChainSaw",dini_Int(PlayerFile(ChainSawWinnerName),"ChainSaw")+1);
FAC_GivePlayerMoney(ChainSawWinner,ChainSawReward);
SendClientMessageToAll(COLOR_WHITE, "<< ~~~~~~ Organized ChainSaw System ~~~~~~ >>");
format(String, sizeof(String), "! %s (id:%d) :המנצח בפעילות המסורים ",ChainSawWinnerName,ChainSawWinner);
SendClientMessageToAll(0xD5F60FF, String);
format(String, sizeof(String), "%d $ : פרס", ChainSawReward);
SendClientMessageToAll(0xD5F60FF, String);
SendClientMessageToAll(COLOR_WHITE, "<< ~~~~~~ להתראות בפעילות הבאה ~~~~~~ >>");
format(String, sizeof(String), ".$%d - זכית ב, ChainSaw -ניצחת בפעילות ה", ChainSawReward);
SendClientMessage(ChainSawWinner, COLOR_LIGHTBLUEGREEN, String);
for(new i;i<GetMaxPlayers();i++){
if(IsPlayerConnected(i))InChainSaw[i] = 0;
}
ChainSawOn = 0,ChainSawPlayers = 0,ChainSawStarted = 0;
KillTimer(ChainSawWinnerCheck);
return 1;
}

else if(status == 2) {
ChainSawPlayers = 0;
ChainSawStarted = 0;
ChainSawOn = 0;
KillTimer(ChainSawWinnerCheck);
for(new i;i<GetMaxPlayers();i++){
if(IsPlayerConnected(i)){
return InChainSaw[i] = 0;
}}
}
return 1;
}
