public ChainSawCheck() {

for(new i;i<GetMaxPlayers();i++){
if(ChainSawStarted == 1 && IsPlayerConnected(i) && InChainSaw[i] == 1){
if(!isPlayerInArea(i,ActivityArea[1])){
SetPlayerHealth(i,0);
}}}

if(ChainSawStarted == 1 && ChainSawPlayers == 1){
for(new i;i<GetMaxPlayers();i++){
if(IsPlayerConnected(i) && InChainSaw[i] == 1){
ChainSawWinner = i;
ChainSawEnd(1);
}}}
return 1;
}



