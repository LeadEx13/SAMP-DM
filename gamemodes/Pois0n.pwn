	if(iRace_Players < MinPlayers_iRace)
	{
	Reset_iRace();
	return SendClientMessageToAll(COLOR_LIGHTGREEN, "- iRace: המירוץ הסתיים בשל חוסר משתתפים");
	}
	for(new seat = 0; seat < iRace_Players; seat++)
	{
	    Vehicle_iRace[seat] = CreateVehicle(411,Infernus_Pos[seat][0],Infernus_Pos[seat][1],Infernus_Pos[seat][2],Infernus_Pos[seat][3],iRace_Color,iRace_Color,999999);
	    AddVehicleComponent(Vehicle_iRace[seat],1010);
	}
	Count_iRace = 0;
	for(new i; i < 200; i++)
	{
		if(IsPlayerConnected(i) && In_iRace[i] == 1)
		{
			PutPlayerInVehicle(i,Vehicle_iRace[Count_iRace],0);
			Player_iRace[i] = Count_iRace;
		    Count_iRace++;
			ResetPlayerWeapons(i);
			SetPlayerHealth(i, 100.0);
			TogglePlayerControllable(i, 0);
            SetPlayerInterior(i,0);
       	    SetPlayerRaceCheckpoint(i,0,Infernus_CheckPoints[0][0],Infernus_CheckPoints[0][1],Infernus_CheckPoints[0][2],Infernus_CheckPoints[1][0],Infernus_CheckPoints[1][1],Infernus_CheckPoints[1][2],50);
		}
	}
	return 1;
}

