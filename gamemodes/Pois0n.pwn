    GetPlayerName(playerid,playername,sizeof(playername));
    if(dini_Int(PlayerFile(playername), "Sweapon2") != 0)LACY_GiveWeapon(playerid,dini_Int(PlayerFile(playername), "Sweapon2"),1000);
    if(dini_Int(PlayerFile(playername), "Sweapon1") != 0)LACY_GiveWeapon(playerid,dini_Int(PlayerFile(playername), "Sweapon1"),1);
    if(dini_Int(PlayerFile(playername), "Teargas") != 0)LACY_GiveWeapon(playerid,17,dini_Int(PlayerFile(playername), "Teargas"));
    if(dini_Int(PlayerFile(playername), "Sniper") != 0)LACY_GiveWeapon(playerid,34,dini_Int(PlayerFile(playername), "Sniper"));
    if(dini_Int(PlayerFile(playername), "Ak/M4 Id") != 0) LACY_GiveWeapon(playerid,dini_Int(PlayerFile(playername), "Ak/M4 Id"),dini_Int(PlayerFile(playername), "Ak47") + dini_Int(PlayerFile(playername), "M4"));
    if(dini_Int(PlayerFile(playername), "Uzi Id") != 0) LACY_GiveWeapon(playerid,dini_Int(PlayerFile(playername), "Uzi Id"),dini_Int(PlayerFile(playername), "Tec9")+dini_Int(PlayerFile(playername), "Uzi")+ dini_Int(PlayerFile(playername), "Mp5"));
    if(dini_Int(PlayerFile(playername), "Shotgun Id") != 0)LACY_GiveWeapon(playerid,dini_Int(PlayerFile(playername), "Shotgun Id"),dini_Int(PlayerFile(playername), "SawnOff") + dini_Int(PlayerFile(playername), "Combat"));
    if(dini_Int(PlayerFile(playername), "Pistol Id") != 0)LACY_GiveWeapon(playerid,dini_Int(PlayerFile(playername), "Pistol Id"),dini_Int(PlayerFile(playername), "Colet45") + dini_Int(PlayerFile(playername), "Silenced"));
    return 1;
}
