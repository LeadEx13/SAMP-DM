forward Teleport_Cycle();
public Teleport_Cycle()
{
	if(Cycle_Players < MinPlayers_Cycle)
	{
	Reset_Cycle();
	return SendClientMessageToAll(COLOR_LIGHTGREEN, "- Cycle: המירוץ הסתיים בשל חוסר משתתפים");
	}
	for(new seat = 0; seat < Cycle_Players; seat++)
	{
      Vehicle_Cycle[seat] = CreateVehicle(510,Cycle_PPPOSS[seat][0],Cycle_PPPOSS[seat][1],Cycle_PPPOSS[seat][2],Cycle_PPPOSS[seat][3],Cycle_Color,Cycle_Color,999999);
      //AddVehicleComponent(Vehicle_Cycle[seat],1010);
	}
	Count_Cycle = 0;
	for(new i; i < 200; i++)
	{
		if(IsPlayerConnected(i) && In_Cycle[i] == 1)
		{
			PutPlayerInVehicle(i,Vehicle_Cycle[Count_Cycle],0);
			Player_Cycle[i] = Count_Cycle;
		    Count_Cycle++;
			ResetPlayerWeapons(i);
			SetPlayerHealth(i, 100.0);
			TogglePlayerControllable(i, 0);
       	    SetPlayerRaceCheckpoint(i,0,CCJeeps_CheckPoints[0][0],CCJeeps_CheckPoints[0][1],CCJeeps_CheckPoints[0][2],CCJeeps_CheckPoints[1][0],CCJeeps_CheckPoints[1][1],CCJeeps_CheckPoints[1][2],50);
       	    SetPlayerInterior(i,0);
		}
	}
	return 1;
}

