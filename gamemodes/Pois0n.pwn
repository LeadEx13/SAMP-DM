		        case 3:
		        {
		            CreateExplosion(AsPos[playerid][0],AsPos[playerid][1]+2,AsPos[playerid][2],7,20);
		        	StrikeProgress[playerid]++;
		        	return SetTimerEx("AirStrikeExplosions",500,0,"i",playerid);
		        }
