				}
				oldPos[i][0] = X;
				oldPos[i][1] = Y;
				oldPos[i][2] = Z;
			}
		}
		time--;
		SetTimerEx("RunPiramidaActivity",1000,false,"i",time);
	}
	else
	{
		new Float:max_high = 0, playerid = 0;
		for (i=0; i<MAX_PLAYERS; i++)
		{
		    if (IsPlayerConnected(i) && players[i])
		    {
		        SetPlayerWorldBounds(i, 20000.0000,-20000.0000,20000.0000,-20000.0000);
		        GetPlayerPos(i,X,Y,Z);
