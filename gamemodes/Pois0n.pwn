public OnPlayerPrivmsg(playerid, recieverid, text[])
{
    GetPlayerName(playerid, playername, sizeof(playername));
    GetPlayerIp(playerid, playerip, sizeof(playerip));
    new recievername[MAX_PLAYER_NAME],recieverip[16];
    GetPlayerName(recieverid,recievername, sizeof(recievername));
    GetPlayerIp(playerid,recieverip, sizeof(recieverip));
    if(!IsPlayerXAdmin(playerid) && PmASpam[playerid] >= 1)return SendClientMessage(playerid, red,"> Pm Anti Spam - הודעה פרטית כל 3 שניות *"),0;
    PmASpam[playerid]++;
    SetTimerEx("PmSpamUpdate",3000,0,"d",playerid);
    if(!IsVaildText(text))
	{
	format(String, sizeof(String), "- ASA - %s (id: %d) tried to pm: %s", playername, playerid, text);
	SendClientMessageToAdmins(COLOR_KRED,String);
	SendClientMessage(playerid,COLOR_GREY,".הודעה זו לא נשלחה מכיוון שזוהתה כפוטנציאלית לפרסום שרתים");
	return false;
	}
    if(BlockPm[playerid] == 1)return SendClientMessage(playerid,COLOR_BRIGHTRED,"[BlockPms] .ההודעות הפרטיות חסומות אצלך "),0;
    if(BlockPm[recieverid] == 1)return SendClientMessage(playerid,COLOR_BRIGHTRED,"[BlockPms] .המשתמש חסם את ההודעות הפרטיות שלו "),0;
    if(dini_Exists(PlayerFile(playername)) && strcmp(dini_Get(PlayerFile(playername),"Organization"),"Supporters",true)){
    if(FAC_GetPlayerMoney(playerid) < PmCost) return SendClientMessage(playerid,COLOR_WHITE,".אין בידך מספיק כסף "),0;
    FAC_GivePlayerMoney(playerid,-PmCost),FAC_GivePlayerMoney(recieverid,PmCost);
    }
 if (Ignore[recieverid][playerid])
	{
		SendClientMessage(playerid,RED," ! השחקן שאתה מנסה לשלוח לו הודעה פרטית מתעלם מההודעות שלך ");
		return false;
	}
    format(String,sizeof(String),"[PM_Out] %s (iD:%d):%s",recievername,recieverid,text),SendClientMessage(playerid,lgreen,String);
    //format(String,sizeof(String),"_____________IncominG PM_____________",playername,playerid,text),SendClientMessage(recieverid,COLOR_BLUEWHITE,String);
format(String,sizeof(String),"[PM_iN] %s (iD:%d):%s",playername,playerid,text),SendClientMessage(recieverid,COLOR_YELLOW,String);
GameTextForPlayer(recieverid,"~y~ IncominG PM !",3000,4);
  // format(String,sizeof(String),"_____________IncominG PM_____________",playername,playerid,text),SendClientMessage(recieverid,COLOR_BLUEWHITE,String);
    // ===== Server PM Log ==== //
    format(String,sizeof(String),"/r/n[IP: %s] %s (id:%d) - To - [IP:%s] %s (id:%d): %s",playerip,playername,playerid,recieverip,recievername,recieverid,text);
    WriteToServerLog("Pms",String);
    // ===== Server PM Log ==== //
    return 0;
}
